{
  "manifest_version": 2,
  "name": "Luminus Filetree",
  "description": "Filetree view for Luminus files",
  "version": "1.0.2",
  "icons": {
    "96": "icon96.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "{7b586319-48f7-4b34-ba64-e2c9d0d39ed2}",
      "update_url": "https://raw.githubusercontent.com/hidingmode/luminus-filetree/master/firefox-update-manifest.json"
    }
  },
  "update_url": "https://raw.githubusercontent.com/hidingmode/luminus-filetree/master/chrome-update-manifest.xml",
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "*://luminus.nus.edu.sg/modules/*",
    "*://luminus.azure-api.net/*"
  ],
  "background": {
    "scripts": ["browser-polyfill.min.js", "background.js"]
  }
}